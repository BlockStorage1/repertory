{
    "configurations": {
        "UnixDebug": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "request": "launch",
                "program": "${workspaceRoot}/build/debug/repertory",
                "args": ["-f", "/home/sgraves/mnt"],
                "cwd": "${workspaceRoot}/build/debug",
                "environment": [],
                "externalConsole": true,
                "MIMode": "gdb",
                "stopAtEntry": true,
                "logging": {
                    "engineLogging": false
                }
            }
        },
        "UnixDebugTest": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "request": "launch",
                "program": "${workspaceRoot}/build/debug/unittests",
                "args": ["--gtest_filter=file_manager.can_close_after_download_timeout"],
                "cwd": "${workspaceRoot}/build",
                "environment": [],
                "externalConsole": true,
                "MIMode": "gdb",
                "stopAtEntry": true,
                "logging": {
                    "engineLogging": false
                }
            }
        }
    }
}
