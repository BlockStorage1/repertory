{
  "configurations": {
    "UnixDebug": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/build/debug/repertory",
        "args": ["-f", "-sk", "./skynet_mount"],
        "cwd": "${workspaceRoot}/build/debug",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
